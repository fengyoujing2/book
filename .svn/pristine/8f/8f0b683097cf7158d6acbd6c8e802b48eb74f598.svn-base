                function ShowMsg(msg) {
                    var str = "<div class='information_box'>";
                    str += "<div class='information_border'>";
                    str += "<div class='information_Info'>";
                    str += msg + "</div></div></div>";
                    $("body").append(str);
                    $(".information_box").show(0, function () {

                            $(".information_box").click(function () {
                                $(".information_box").hide(0);
                            });
                    });
                }

                flash();

                /* 提交 */
                function flash(){
                    $("#load").show();
                    var name= $("#name").val();
                    var telephone=$("#telephone").val();
                    /* 验证码 */
                    var yanzheng = $("#yanzheng").val();
                    var money=$("#slider-0 option:selected").val();
                    var path = getRealPath();
                    var url9 = location.search;
                    var theRequest = new Object();
                    if (url9.indexOf("?") != -1) {
                        var str9 = url9.substr(1);
                        strs = str9.split("&");
                        for(var i = 0; i < strs.length; i ++) {
                            theRequest[strs[i].split("=")[0]]=unescape(strs[i].split("=")[1]);
                        }
                    }

                    var WXCode=theRequest.code;
                    
					/* code:验证码；YBWY0007_TP：客户来源；WXCode：授权后传来的code */
                    jQuery.ajax({
                        url:""+path+"/wc_wbybc/getUMsg",
                        data:{YBWY0002:name,YBWY0003:telephone,YBWY0005:money,code:yanzheng,YBWY0007_TP: YBWY0007_TP,WXCode:WXCode},
                        type:'POST',
                        dataType:"json",
                        async: false,
                        success:function(result,status){
                            //result.back:是服务器返回的值。1代表手机验证匹配错误。0代表手机验证匹配成功
                            if(result.msg == "200"){
                            	_taq.push({convert_id:"75733460934", event_type:"form"})
                                //跳转页面
                                $("#load").hide();
                                $("#success").show();
                            }else if(result.msg == "503"){
                                $("#load").hide();
                                $("#feedback").show(0,function(){
                                    $("#error_show").text("手机验证码输入错误");
                                });
                            }else if(result.msg == "502"){
                            	 $("#load").hide();
                                 $("#feedback").show(0,function(){
                                     $("#error_show").text("验证码过期");
                                 });
                            }else if(result.msg == "504"){
	                           	 $("#load").hide();
	                             $("#feedback").show(0,function(){
	                                 $("#error_show").text("内部异常");
	                             });
                       		 }
                        },
                        error:function(result,status){
                            //提示错误信息
                            // parsererror:已发送成功，返回的时候解析错误
                            // error：发送失败，没有发送过去
                            // timeout：超时
                            if(status=="parsererror"){
                                $("#load").hide();
                                $("#feedback").show(0,function(){
                                    $("#error_show").text("解析出错");
                                });
                            }else if(status=="error"){
                                $("#load").hide();
                                $("#feedback").show(0,function(){
                                    $("#error_show").text("信息提交失败");
                                });
                            }else if(status=="timeout"){
                                $("#load").hide();
                                $("#feedback").show(0,function(){
                                    $("#error_show").text("发送超时");
                                });
                            }else{
                                $("#load").hide();
                                $("#feedback").show(0,function(){
                                    $("#error_show").text("发送出错");
                                });
                            }

                        }
                    });

                }
            });

        });
    </script>


                            $(".information_box1").click(function () {
                                $(".information_box1").hide(0);
                            });

                    });

                }

                var path = getRealPath();
                var telephone1=$("#telephone").val();
                //限制获取手机验证码按钮，只能点击三次

                /* 获取验证码 */
                $.post(""+path+"/wc_wbybc/sendCode.do",{YBWY0003:telephone1},function(data){
                   //今日获取验证码次数已达上限！
                   if(data == "501" || data == "505"){
                	   //alert("已达上限");
                       $(".information_box2").show();
                       if(data == "501"){
                    	   $(".information_Info2 ").text("今日获取验证码次数已达上限！");
                       }else if(data == "505"){
                    	   $(".information_Info2 ").text("该IP获取验证码次数已达上限！");
                       }
                       $("#btn10").show();
                       $("#btn").remove();
                       $("#ant").remove();
                       $("#ant2").remove();
                	   //将获取验证码置灰并且给予提示
                   }else if(data == "200"){
                	   //alert("发送成功");
//                       $(".information_box2").show();
//                       $(".information_Info2 ").text("验证码发送成功！");
                	   //验证码发送成功
                   }
                });

                $("#btn").hide();
                $("#ant").text("60s");

                var odiv = document.getElementById("ant");
                var count = 60;
                var timer = null;
                timer = setInterval(function () {
                    if (count > 0) {
                        count = count - 1;
                        odiv.innerHTML = count+"s";
                    }
                    else {
                        clearInterval(timer);
                        $("#ant").text("");
                        $("#btn_inner").text("重新获取");
                        $("#btn").show();


                    }
